
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Anime {
  id            Int     @id @default(autoincrement())
  title         String
  titleJapanese String
  imageURL      String
  synopsis      String
  episodes      Int
  status        String
  score         Float
  year          Int
}